{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "bussiness-owners": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "projects/bussiness-owners",
      "sourceRoot": "projects/bussiness-owners/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "baseHref": "/bussinessowners/",
            "outputPath": "dist/bussiness-owners",
            "index": "projects/bussiness-owners/src/index.html",
            "browser": "projects/bussiness-owners/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/bussiness-owners/tsconfig.app.json",
            "assets": [
              "projects/bussiness-owners/src/favicon.ico",
              "projects/bussiness-owners/src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/bootstrap/scss/bootstrap.scss",
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "projects/bussiness-owners/src/styles.css"
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "50mb",
                  "maximumError": "50mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/bussiness-owners/src/environments/environment.ts",
                  "with": "projects/bussiness-owners/src/environments/environment.development.ts"
                }
              ]
            },
            "stage": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/bussiness-owners/src/environments/environment.ts",
                  "with": "projects/bussiness-owners/src/environments/environment.stage.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "bussiness-owners:build:production"
            },
            "development": {
              "buildTarget": "bussiness-owners:build:development"
            },
            "stage": {
              "buildTarget": "bussiness-owners:build:stage"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "bussiness-owners:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/bussiness-owners/tsconfig.spec.json",
            "assets": [
              "projects/bussiness-owners/src/favicon.ico",
              "projects/bussiness-owners/src/assets"
            ],
            "styles": ["projects/bussiness-owners/src/styles.css"],
            "scripts": []
          }
        }
      }
    },
    "adminportal": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "projects/adminportal",
      "sourceRoot": "projects/adminportal/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "baseHref": "/adminportal/",
            "outputPath": "dist/adminportal",
            "index": "projects/adminportal/src/index.html",
            "browser": "projects/adminportal/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/adminportal/tsconfig.app.json",
            "assets": [
              "projects/adminportal/src/favicon.ico",
              "projects/adminportal/src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/bootstrap/scss/bootstrap.scss",
              "node_modules/bootstrap-icons/font/bootstrap-icons.css",
              "projects/adminportal/src/styles.css"
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "50mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "adminportal:build:production"
            },
            "development": {
              "buildTarget": "adminportal:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "adminportal:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/adminportal/tsconfig.spec.json",
            "assets": [
              "projects/adminportal/src/favicon.ico",
              "projects/adminportal/src/assets"
            ],
            "styles": ["projects/adminportal/src/styles.css"],
            "scripts": []
          }
        }
      }
    },
    "shared-lib": {
      "projectType": "library",
      "root": "projects/shared-lib",
      "sourceRoot": "projects/shared-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/shared-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/shared-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/shared-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/shared-lib/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "microtec-auth-lib": {
      "projectType": "library",
      "root": "projects/microtec-auth-lib",
      "sourceRoot": "projects/microtec-auth-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/microtec-auth-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/microtec-auth-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/microtec-auth-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/microtec-auth-lib/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    }
  }
}
